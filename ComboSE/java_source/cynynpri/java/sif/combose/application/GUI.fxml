<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="532.0" prefWidth="595.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cynynpri.java.sif.combose.application.GUIControler">
   <top>
      <TabPane prefHeight="514.0" prefWidth="595.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab fx:id="devicesettingsTab" text="デバイス設定">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="466.0" prefWidth="595.0">
                     <children>
                        <Spinner fx:id="deviceIndex_spn" editable="false"  layoutX="14.0" layoutY="41.0" min="0" max="10" initialValue="0" />
                        <Label layoutX="102.0" layoutY="24.0" text="デバイス番号" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="windowsettingsTab" text="子ウインドウ設定">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab fx:id="detectsettingsTab" text="コンボ数値読みとり設定">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </top>
   <bottom>
      <Button fx:id="showwindowbtn" mnemonicParsing="false" onMouseClicked="#shownewStage" text="子ウインドウを表示" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
